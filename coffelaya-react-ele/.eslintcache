setDiscountPrice] = useState(0);\n    const [creditPrice, setCreditPrice] = useState(0);\n\n\n\n\n    let totalPrice = 0;\n\n\n    let itemsTotals = items.map((item) => {\n        return (item.getTotal());\n    });\n\n    if (items.length == 1) {\n        totalPrice = itemsTotals[0];\n    } else if (items.length > 1) {\n        totalPrice = sumPrice(...itemsTotals);\n    }\n\n\n    let amountPrice = totalPrice - discountPrice;\n    let paidPrice = amountPrice - creditPrice;\n\n    //Input data from bill table\n    function handleRowInput(inputRow) {\n        setItems([...items, inputRow]);\n\n    }\n\n    function handleDiscountInput(inpDiscountPrice) {\n        setDiscountPrice(inpDiscountPrice);\n    }\n\n    function handleCreditInput(inpCreditPrice) {\n        setCreditPrice(inpCreditPrice);\n    }\n\n    function sumPrice(...args) {\n        return args.reduce((prev, curr) => {\n            return prev + curr;\n        });\n    }\n\n\n    <div id='creditName'>\n        <label htmlFor='creditName'><strong><b> Creditor:</b></strong></label><br />\n        <input\n            type='text'\n            id='creditNameInp'\n            disabled\n\n        />\n    </div>\n    return (\n        <>\n            {/* <Board /> */}\n            <BillTable\n                orderList={items}\n                addRowInput={handleRowInput}\n            />\n            <BillDetail\n                totalPrice={totalPrice}\n                getDiscount={handleDiscountInput}\n                getCredit={handleCreditInput}\n                discountPrice={discountPrice}\n                creditPrice={creditPrice}\n                amountPrice={amountPrice}\n                paidPrice={paidPrice}\n            />\n            <DoneButton />\n\n        </>\n\n    );\n}\n\nexport default Billing;","/home/unchhohang/Documents/my-apps/coffelaya-react-ele/src/page/Credit.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-react-ele/src/compo/App.js",[],"/home/unchhohang/Documents/my-apps/coffela