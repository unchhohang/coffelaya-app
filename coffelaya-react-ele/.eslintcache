redit'>\n                <UnpaidCredit\n                    debtorName={props.debtorName}\n                />\n            </div>\n        </>\n\n    );\n}\n\nexport default CreditDetails;","/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/compo/report/paidCredit.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/compo/report/unpaidCredit.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/page/Menu.js",["391","392","393","394","395"],"import { useEffect, useState } from \"react\"\nimport { Button, Table } from \"react-bootstrap\"\nimport axios from 'axios'\nimport './Menu.css'\nimport MenuCard from \"../compo/menus/menuCard\"\nimport AddTitleBtn from \"../compo/menus/AddTitleBtn\"\n\nconst Menu = (props) => {\n    const [menu, setMenu] = useState([]);\n\n    useEffect(() => {\n        getMenu();\n    }, []);\n\n    function getMenu() {\n        axios.get('/api/menu')\n            .then(data => {\n                setMenu(data.data);\n                console.log('got data:');\n                console.log(data.data);\n\n            })\n            .catch(err => console.log(err))\n    }\n\n    function insertMenuTitle(title) {\n        let url = '/api/menu'\n\n        if (title == '') {\n            return;\n        }\n\n        axios.post(url, {\n            'title': title\n        })\n            .then(\n                (data) => {\n                    getMenu();\n\n                }\n            )\n            .catch(err => console.log(err))\n    }\n\n\n\n    //if empty title show\n    if (menu.length != 0) {\n\n        let menuCardList = menu.map((mCard, i) => {\n            return (\n                <div\n                    key={i}\n                >\n                    <MenuCard\n                        insertMenuTitle={insertMenuTitle}\n                        mCard={mCard}\n                        isLast={i == menu.length - 1}\n                        getMenu={getMenu}\n    