    let totalCredit;\n\n    useEffect(() => {\n        getDebtorDetail(debtorName)\n    }, [debtorName]);\n\n    function getDebtorDetail(name) {\n        let url = '/credit/debtorByName/' + name\n        axios.get(url)\n            .then((data) => { setDebtorDetail(data.data) })\n            .catch(err => console.log(err))\n\n    }\n\n    function setterDebtorName(){\n        getDebtorDetail(debtorName);\n    }\n\n\n\n    let creditList = debtorDetail.map((ele) => {\n        return (ele.creditAmount);\n    });\n\n\n    if (creditList.length == 1) {\n        totalCredit = creditList[0];\n    } else if (creditList.length > 1) {\n        totalCredit = sumPrice(...creditList);\n    }\n\n    return (\n        (debtorDetail.length == 0) ? <h1>Select Debtor</h1>\n            :\n            <>\n                <CreditDesc\n                    debtorName={debtorName}\n                    totalCredit={totalCredit}\n                    debtorBills={debtorDetail}\n                    refreshCredit={props.refreshCredit}\n                    setterDebtorName={setterDebtorName}\n                />\n            </>\n    );\n}\n\nexport default CreditMain;","/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/compo/credit/creditDesc.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/js functions/sum.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/compo/credit/creditBills.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/compo/credit/billsView.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/compo/credit/viewTable.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/js functions/payCreditBill.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/js functions/sortCredit.js",["342"],"function sortCredit(billList) {\n    billList.sort(\n        function (a, b) {\n            { return b.creditAmount - a.creditAmount; }\n        }\n    );\n    return billList\n}\n\nexport default sortCredit;","/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/page/Entries.js",["343","344"],"import { useState, useEffect } from 'react'\nimport DatePicker from '../compo/entries/datePicker'\nimport axios from 'axios'\nimport EntrieBills from '../compo/entries/entrieBills'\nimport nepToEngDate from '../js functions/nepToEngDate'\n\nconst Entries = () => {\n    const [date, setDate] = useState();\n    const [billList, setBillList] = useState();\n\n    useEffect(() => {\n        let todayDate = new Date();\n        let [startDate, endDate] = nepToEngDate(todayDate);\n\n        getEntries(startDate, endDate);\n    }, []);\n\n\n\n    function selectDate(valueDate) {\n        setDate(valueDate);\n\n        let [startDate, endDate] = nepToEngDate(valueDate);\n        getEntries(startDate, endDate);\n\n    }\n\n    function refreshEntries() {\n        if (date == null) {\n            let todayDate = new Date();\n            let [startDate, endDate] = nepToEngDate(todayDate);\n            getEntries(startDate, endDate);\n        } else {\n            let [startDate, endDate] = nepToEngDate(date);\n            getEntries(startDate, endDate);\n\n        }\n\n\n    }\n\n\n\n    function getEntries(startDate, endDate) {\n        let url = '/entries/' + startDate + '/' + endDate;\n        axios.get(url)\n            .then(data => setBillList(data.data))\n            .catch(err => { console.log(err) })\n    }\n\n    function deleteEntries(billId) {\n        let url = '/api/billing/' + billId;\n\n        axios.get(url)\n            .then(data => {\n                let debtorName = data.data.debtorName;\n\n                removeEntriesFromCollection(billId, debtorName);\n            })\n            .catch()\n\n\n    }\n\n    function removeEntriesFromCollection(billId, debtorName) {\n        let url = '/api/billing/' + billId;\n\n        axios.delete(url)\n            .then(() => {\n                segregateMoneyGiven(debtorName)\n                refreshEntries();\n\n            })\n            .catch(err => console.log(err))\n    }\n\n    function segregateMoneyGiven(debtorName) {\n        axios.get('/api/debtors')\n            .then(data => {\n                let debtorList = data.data\n\n                if (debtorList.includes(debtorName) == false) {\n                    let url = '/credit/moneyGiven/' + debtorName;\n\n                    axios.delete(url)\n                        .then(data => console.log('debtor is deleted from MoneyGiven collection : ' + debtorName))\n                        .catch(err => console.log(err));\n                }\n            })\n\n    }\n\n    return (\n        <div className='row'>\n            <div className='col-sm-6'></div>\n            <div className='col-sm-1'>Date:</div>\n            <div className='col-sm-4'>\n                <DatePicker\n                    date={date}\n                    selectDate={selectDate}\n                />\n            </div>\n            <div>\n                {\n                    (billList != undefined)\n                        ? <EntrieBills\n                            billList={billList}\n                            date={date}\n                            deleteEntries={deleteEntries}\n                        />\n                        : <h1>No Entries to show</h1>\n                }\n            </div>\n        </div>\n\n    );\n\n}\n\nexport default Entries;","/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/compo/entries/datePicker.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/compo/entries/entrieBills.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/js functions/nepToEngDate.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/compo/stock/stockTable.js",["345","346","347","348","349","350"],"import { Table } from