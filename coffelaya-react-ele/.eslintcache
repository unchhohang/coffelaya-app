   const [creditPrice, setCreditPrice] = useState(0);\n\n\n\n\n    let totalPrice = 0;\n\n\n    let itemsTotals = items.map((item) => {\n        return (item.getTotal());\n    });\n\n    if (items.length == 1) {\n        totalPrice = itemsTotals[0];\n    } else if (items.length > 1) {\n        totalPrice = sumPrice(...itemsTotals);\n    }\n\n\n    let amountPrice = totalPrice - discountPrice;\n    let paidPrice = amountPrice - creditPrice;\n\n    //Input data from bill table\n    function handleRowInput(inputRow) {\n        setItems([...items, inputRow]);\n\n    }\n\n    function handleDiscountInput(inpDiscountPrice) {\n        setDiscountPrice(inpDiscountPrice);\n    }\n\n    function handleCreditInput(inpCreditPrice) {\n        setCreditPrice(inpCreditPrice);\n    }\n\n    function sumPrice(...args) {\n        return args.reduce((prev, curr) => {\n            return prev + curr;\n        });\n    }\n\n\n    <div id='creditName'>\n        <label htmlFor='creditName'><strong><b> Creditor:</b></strong></label><br />\n        <input\n            type='text'\n            id='creditNameInp'\n            disabled\n\n        />\n    </div>\n    return (\n        <>\n            {/* <Board /> */}\n            <BillTable\n                orderList={items}\n                addRowInput={handleRowInput}\n            />\n            <BillDetail\n                totalPrice={totalPrice}\n                getDiscount={handleDiscountInput}\n                getCredit={handleCreditInput}\n                discountPrice={discountPrice}\n                creditPrice={creditPrice}\n                amountPrice={amountPrice}\n                paidPrice={paidPrice}\n            />\n            <DoneButton />\n\n        </>\n\n    );\n}\n\nexport default Billing;","/home/unchhohang/Documents/my-apps/coffelaya-react-ele/src/page/Credit.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-react-ele/src/compo/App.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-react-ele/src/nav/DropDown.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-react-ele/src/page/Report.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-react-ele/src/page/Stocks.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-react-ele/src/compo/bill/TopBoard.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-react-ele/src/compo/bill/BillTable.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-react-ele/src/class/ItemClass.js",[],["307","308"],"/home/unchhohang/Documents/my-apps/coffelaya-react-ele/src/compo/bill/BillDetail.js",["309"],"import { useState } from \"react\"\nimport { Col, Row } from \"react-bootstrap\"\nimport './BillDetail.css'\nimport BillInfo from './BillInfo'\nimport BillInfoCredit from './BillInfoCredit'\nimport './SliderCss.css'\n\nconst BillDetail = (props) => {\n\n    console.log(\"rendered BillDetail\");\n\n    function onDiscountChange(){\n        let discount = document.getElementById('discountInp');\n        props.getDiscount(discount.value);\n    }\n\n    function onCreditChange(){\n        let credit = document.getElementById('creditInp');\n        props.getCredit(credit.value);\n    }\n\n    function onChangeCheckbox() {\n        let checkSwitch = document.getElementById('creditMode');\n        if (checkSwitch.checked) {\n            document.getElementById('creditInp').removeAttribute(\"disabled\")\n            document.getElementById('billInfo').style.display = \"none\";\n            document.getElementById('billInfoCredit').style.display = \"\";\n            document.getElementById('creditNameInp').removeAttribute(\"disabled\");\n\n        } else {\n            document.getElementById('creditInp').setAttribute(\"disabled\", \"\");\n            document.getElementById('billInfo').style.display = \"\";\n            document.getElementById('billInfoCredit').style.display = \"none\";\n            document.getElementById('creditNameInp').setAttribute(\"disabled\", \"\");\n\n\n        }\n    }\n\n    return (\n        <div id='billDetail'>\n            <Row>\n                <Col>\n                    <div id='DandC'>\n                        <div id='discount'>\n                            <label htmlFor='discount'><strong><b>Discount:</b></strong></label><br />\n                            <input\n                                type='number'\n                                id='discountInp'\n                                min='0' max='100'\n                            onChange={onDiscountChange}\n                            />\n                            <select id='disType'>\n                                <option value='digit' >Digit</option>\n                                <option value='%' defaultValue>%</option>\n\n                            </select>\n                        </div>\n                        <div id='credit'>\n                            <label htmlFor='credit'><strong><b> Credit Amount:</b></strong></label><br />\n                            <input\n                                type='number'\n                                id='creditInp'\n                                disabled\n                            onChange={onCreditChange}\n\n                            />\n                            <label className='switch'>\n                                <input\n                                    type='checkbox'\n                                    id='creditMode'\n                                    onChange={onChangeCheckbox}\n                                >\n                                </input>\n                                <span className='slider round'></span>\n                            </label>\n                            <div id='creditName'>\n                                <label htmlFor='creditName'><strong><b> Creditor Name:</b></strong></label><br />\n                                <input\n                                    type='text'\n                                    id='creditNameInp'\n                                    disabled\n\n                   