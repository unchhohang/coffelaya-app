torName = props.debtorName;\n    let totalCredit;\n\n    useEffect(() => {\n        getDebtorDetail(debtorName)\n    }, [debtorName]);\n\n    function getDebtorDetail(name) {\n        let url = '/credit/debtorByName/' + name\n        axios.get(url)\n            .then((data) => { setDebtorDetail(data.data) })\n            .catch(err => console.log(err))\n\n    }\n\n    function setterDebtorName(){\n        getDebtorDetail(debtorName);\n    }\n\n\n\n    let creditList = debtorDetail.map((ele) => {\n        return (ele.creditAmount);\n    });\n\n\n    if (creditList.length == 1) {\n        totalCredit = creditList[0];\n    } else if (creditList.length > 1) {\n        totalCredit = sumPrice(...creditList);\n    }\n\n    return (\n        (debtorDetail.length == 0) ? <h1>Select Debtor</h1>\n            :\n            <>\n                <CreditDesc\n                    debtorName={debtorName}\n                    totalCredit={totalCredit}\n                    debtorBills={debtorDetail}\n                    refreshCredit={props.refreshCredit}\n                    setterDebtorName={setterDebtorName}\n                />\n            </>\n    );\n}\n\nexport default CreditMain;","/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/compo/credit/creditDesc.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/js functions/sum.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/compo/credit/creditBills.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/compo/credit/billsView.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/compo/credit/viewTable.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/js functions/payCreditBill.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/js functions/sortCredit.js",["315"],"function sortCredit(billList) {\n    billList.sort(\n        function (a, b) {\n            { return b.creditAmount - a.creditAmount; }\n        }\n    );\n    return billList\n}\n\nexport default sortCredit;","/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/page/Entries.js",["316","317"],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/compo/entries/datePicker.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/compo/entries/entrieBills.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/js functions/nepToEngDate.js",[],"/home/unchhohang/Documents/my-apps/coffelaya-mern/coffelaya-react-ele/src/compo/stock/stockTable.js",["318","319","320","321","322","323"],"import { Table } from 'react-bootstrap'\nimport sumPrice from '../../js functions/sum'\nimport { Button } from 'react-bootstrap'\nimport axios from 'axios'\nimport { useEffect } from 'react'\nimport engToIsoDate from '../../js functions/engToIsoDate'\nimport nepToEngDate from '../../js functions/nepToEngDate'\n//following css is Stock.css\n\nconst StockTable = (props) => {\n\n    let stock = props.stock;\n    let date = props.date;\n    let totalCost = 0;\n    let engDate;\n    let tommorowEngDate;\n\n    useEffect(() => {\n        if (date == undefined) {\n            date = new Date();\n            engDate = engToIsoDate(date);\n        } else { \n            [engDate, tommorowEngDate] = nepToEngDate(date);\n        }\n    });\n\n    console.log('date inside stock table: ' + date);\n\n    function deleteStock(stockId) {\n        let url = '/stock/' + stockId\n        axios.delete(url)\n            .then(props.refreshStock(engDate))\n            .catch(err => console.log(err))\n    }\n\n    let tbody = stock.map((ele, i) => {\n        return (\n            <tr key={i}>\n                <td>{i + 1}</td>\n                <td>{ele.stock}</td>\n                <td>{ele.cost}</td>\n                <td><Button\n                    variant=\"danger\"\n                    si